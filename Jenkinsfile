pipeline {
    agent any
    
    stages {
       stage('Clone') {
           steps { 
              echo 'Coloning main Branch ....'
            } 
       }    
       stage('Build') {
         steps { 
             echo 'Building main Branch ....'
         }
       }
       stage('Run') {
          steps { 
             echo 'Runing main Branch ....'
         }
       }
   }
}
